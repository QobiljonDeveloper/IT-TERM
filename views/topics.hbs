<h1 style="text-align: center;">{{title}}</h1>

<div
  class="container"
  style="display: flex; gap: 15px; flex-wrap: wrap; margin-top: 20px;"
  id="container"
>
  
</div>



<script>
  const container = document.getElementById("container");

  async function getTopics() {
    let accessToken = localStorage.getItem("accessToken");

    try {
      let response = await fetch("http://localhost:3333/api/topic/", {
        method: "GET",
        headers: {
          Authorization: `Bearer ${accessToken}`,
          "Content-type": "application/json",
        },
      });

      let data = await response.json();
      console.log(data);

      let topics = "";
      data.topics.forEach((val) => {
        topics += `
          <div class="card" style="width: 18rem; margin: 10px;">
          <img
        src="data:image/jpeg;base64,"
        alt="..."
      />
            <div class="card-body">
              <ul class="list-group list-group-flush">
                <li class="list-group-item"><strong>Mavzu:</strong> ${val.topic_title}</li>
                <li class="list-group-item"><strong>To'liq mavzu:</strong> ${val.topic_text}</li>
              </ul>
            <button class="btn btn-${val.is_checked ? "success" : "danger"}" style="margin-top: 10px;">
                ${val.is_checked ? "Active" : "Passive"}
            </button>

            </div>
          </div>
        `;
      });

      container.innerHTML = topics;

    } catch (error) {
      console.log(error.message);
    }
  }

  getTopics();
</script>